{
  "name": "react-kun-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint:stylelint": "stylelint  \"**/*.{css,scss}\" --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cyntler/react-doc-viewer": "^1.16.6",
    "@iconify/react": "^5.0.1",
    "@logicflow/core": "1.2.26",
    "@logicflow/extension": "1.2.26",
    "@pqina/pintura": "^8.82.15",
    "@pqina/react-pintura": "^9.0.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-signature-canvas": "^1.0.5",
    "@uiw/react-md-editor": "^4.0.4",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "@wavesurfer/react": "^1.0.7",
    "antd": "^5.17.4",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "i18next": "^23.11.5",
    "less": "^4.2.0",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-barcode": "^1.5.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.0.7",
    "react-file-viewer": "^1.2.1",
    "react-i18next": "^14.1.2",
    "react-image-crop": "^11.0.6",
    "react-json-view": "^1.21.3",
    "react-pdf": "^9.0.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.23.1",
    "react-signature-canvas": "^1.0.6",
    "react-to-print": "^2.15.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.3",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "terser": "^5.31.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "2.9.6",
    "wavesurfer.js": "^7.8.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
